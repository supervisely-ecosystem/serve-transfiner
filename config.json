{
  "name": "Serve Transfiner",
  "type": "app",
  "version": "2.0.0",
  "description": "Deploy Transfiner for instance segmentation as a REST API service",
  "categories": [
    "neural network",
    "images",
    "videos",
    "instance segmentation",
    "segmentation & tracking",
    "serve"
  ],
  "session_tags": [
    "deployed_nn"
  ],
  "need_gpu": true,
  "community_agent": false,
  "docker_image": "supervisely/detectron2:1.0.3",
  "instance_version": "6.6.7",
  "entrypoint": "python -m uvicorn src.main:m.app --app-dir ./serve --host 0.0.0.0 --port 8000 --ws websockets",
  "port": 8000,
  "task_location": "application_sessions"
}